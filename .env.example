# Configuración de Desarrollo
VITE_API_BASE_URL=http://localhost:3030/api
VITE_WS_URL=ws://localhost:3030/ws
VITE_APP_NAME=Recaudo
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Configuración del Backend - Desarrollo
VITE_BACKEND_HOST=localhost
VITE_BACKEND_PORT=3030
VITE_BACKEND_PROTOCOL=http

# Configuración de WebSocket
VITE_WS_ENDPOINT=/ws/sistema-externo
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT_ATTEMPTS=10

# Configuración de CORS
VITE_CORS_ORIGIN=http://localhost:5174
VITE_CORS_METHODS=GET,POST,PUT,DELETE,OPTIONS
VITE_CORS_HEADERS=Content-Type,Authorization

# Configuración de Timeout
VITE_API_TIMEOUT=10000
VITE_WS_TIMEOUT=30000

# Configuración de Logs
VITE_LOG_LEVEL=debug
VITE_DEBUG_MODE=true

