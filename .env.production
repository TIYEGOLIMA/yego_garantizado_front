# Configuración de Producción
VITE_APP_ENV=production
VITE_API_BASE_URL=https://api-int.yego.pro
VITE_WS_URL=https://api-int.yego.pro/ws
VITE_CORS_ORIGIN=https://neto.yego.pro
VITE_LOG_LEVEL=info
VITE_DEBUG_MODE=false

# Configuración del Backend
VITE_BACKEND_HOST=api-int.yego.pro
VITE_BACKEND_PORT=443
VITE_BACKEND_PROTOCOL=https

# Configuración de WebSocket
VITE_WS_ENDPOINT=/ws/sistema-externo
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT_ATTEMPTS=10

# Configuración de Timeout
VITE_API_TIMEOUT=10000
VITE_WS_TIMEOUT=30000
